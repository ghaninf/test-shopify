<input type="hidden" name="tuecus-customer-id" value="{{ customer.id }}" id="tuecus-customer-id">
<input type="hidden" name="tuecus-app-proxy" value="{{ shop.metafields.tuecus.app_proxy }}" id="tuecus-app-proxy">
