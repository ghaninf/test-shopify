<script>
	if (!window.tuecusObj || typeof window.tuecusObj == "undefined") {
		window.tuecusObj = {};
	}	
	window.tuecusObj.refer_friend = {};
	window.tuecusObj.refer_friend.registration_url = "https://{{ shop.domain }}/account/register";
	window.tuecusObj.refer_friend.refer_message = "Hi Earn credits by registering with {{ shop.name }}. Use this link and get your rewards:";
	
	var earned_rewards_str = 'Earned Rewards';
	{% assign earned_rewards = 'tuecus.general.earned_rewards' | t %}
	{% if  earned_rewards != 'translation missing' %} 
	  earned_rewards_str = '{{ earned_rewards }}';
	{% endif %}
	
	var spent_rewards_str = 'Spent Rewards';
	{% assign spent_rewards = 'tuecus.general.spent_rewards' | t %}
	{% if  spent_rewards != 'translation missing' %} 
	  spent_rewards_str = '{{ spent_rewards }}';
	{% endif %}
	
	var current_rewards_str = 'Current Rewards';
	{% assign current_rewards = 'tuecus.general.current_rewards' | t %}
	{% if  current_rewards != 'translation missing' %} 
	  current_rewards_str = '{{ current_rewards }}';
	{% endif %}
	
	var total_rewards_str = 'Total Rewards';
	{% assign total_rewards = 'tuecus.general.total_rewards' | t %}
	{% if  total_rewards != 'translation missing' %} 
	  total_rewards_str = '{{ total_rewards }}';
	{% endif %}
	
	var no_reviews_str = 'No Reviews';
	{% assign no_reviews = 'tuecus.general.no_reviews' | t %}
	{% if  no_reviews != 'translation missing' %} 
	  no_reviews_str = '{{ no_reviews }}';
	{% endif %}
      
    var do_not_have_reviews_str = 'Looks like you don\'t have any reviews.';
	{% assign do_not_have_reviews = 'tuecus.general.do_not_have_reviews' | t %}
	{% if  do_not_have_reviews != 'translation missing' %} 
	  do_not_have_reviews_str = '{{ do_not_have_reviews }}';
	{% endif %}
	
	var add_to_cart_str = 'ADD TO CART';
	{% assign add_to_cart = 'tuecus.general.add_to_cart' | t %}
	{% if  add_to_cart != 'translation missing' %} 
	  add_to_cart_str = '{{ add_to_cart }}';
	{% endif %}
	
</script>


