     document.getElementById('tuecus-profile-image-modal').style.display='block';
  }

 async function uploadFile() {
     //  alert('uploadFile');
      var formData = new FormData();   
      var profile_pic_file = document.getElementById('profile_pic_file');
    
      var app_id =  document.getElementById('tuecus-app-id').value; 
      var store_token = document.getElementById('tuecus-store-token').value;
	  var customer_id = document.getElementById('tuecus-customer-id').value;
	
      var url="/apps/"+document.getElementById('tuecus-app-proxy').value+"/api/profile/uploadpic/"+document.getElementById('tuecus-store-token').value;;
		
    
      formData.append("app_id",app_id);
      formData.append("store_token", store_token);
      formData.append("customer_id",customer_id);
      formData.append("file", profile_pic_file.files[0]);

	  var ajax = new XMLHttpRequest();
      ajax.onreadystatechange =
	  function(){
			if(ajax.readyState==4 && ajax.status==200){
				var resdata = ajax.responseText;
				var response = JSON.parse(resdata);
				console.log(response);
				document.getElementById('tuecus_avatar').src=response.base64;
				document.getElementById('tuecus-profile-image-modal').style.display='none';
				alert('The file has been uploaded successfully.');

			}  else{
			}
	   }               
       ajax.open("POST",url);
       //ajax.setRequestHeader("Content-Type", "multipart/form-data");
       ajax.send(formData);

      /*await fetch(url, {
        method: "POST", 
        body: formData
      });  
	  document.getElementById('tuecus-profile-image-modal').style.display='none';
      alert('The file has been uploaded successfully.');*/
  }

      
</script>      
